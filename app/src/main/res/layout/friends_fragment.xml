            app:layoutManager="androidx.recyclerview.widget.StaggeredGridLayoutManager"
            android:layout_marginBottom="@dimen/activity_titlebar_margin"
            tools:listitem="@layout/friend_list"/>


    </FrameLayout>

</layout>
